       var lang = $("html").attr("lang").split("-")[0];
       var langState = $("html").attr("lang");

        if(lang!=null){

                 if ( langState == "en-gb"){
                    var labels = delivery_labels[langState];           
                 } else {
                    var labels = delivery_labels[lang];
                 }   


                if(labels!=null){

                        $("[delivery_label]").each(function(){

                                

                                var k = $(this).attr("delivery_label");
                                
                                var v = labels[k];
                                if(v!=null){
                                      if ($(this).attr('value') != undefined && $(this).attr('placeholder') == undefined  ) {
                                          $(this).attr("value", v);   
                                      }
                                     if ($(this).attr('placeholder') != undefined) {
                                          $(this).attr("placeholder", v);   
                                      }
                                        $(this).html(v);
                                        $(this).removeAttr("delivery_label");
                                }
                        });
                } 
        } 
}

